    "template",
    "enzyme",
    "eslint",
    "redux",
    "react-redux",
    "react-router",
    "react-router-dom",
    "typescript",
    "prettier"
  ],
  "files": [
    "template",
    "template.json"
  ],
  "repository": {
    "type": "git",
